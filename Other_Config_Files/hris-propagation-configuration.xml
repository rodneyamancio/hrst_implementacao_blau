            <destination field-id="local-job-title" hris-element-id="jobInfo"/>
        </field>
<!--[EC2MDF MIGRATION] CSF field added for: [jobClassLocal.genericString1], with Field-Id: [JobClassificationAUS.ascoCode], CountryCode: [AUS]-->
